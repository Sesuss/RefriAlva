<div class="container-fluid p-1">
  <div class="row mx-auto">
    <div class="col-md-12">
      <div class="row">

        <div class="col-md-4">
          <img alt="" src="Logo.png" width="760px" height="150px">
        </div>
        <div class="row" style="margin-top: 20px;" >
          <div class="col-md-12">
            <center style="font-size:80px;"><strong style="color: red;">G A R A N T I A</strong></center>
            <address style="font-size:x-large; margin-left: 10px;">
              {{#each datos}}
              <strong style="margin-left: 300px;">Fechas</strong>
              <br><center>
             <strong>Solicitud:</strong> {{moment FechaSolicitud}} &nbsp; &nbsp;
              <strong>Servicio:</strong> {{moment FechaVisita}}
              <br>
             <strong>Recoleccion:</strong> {{moment FechaRecoleccion}} &nbsp; &nbsp;
             <strong>Entrega:</strong> {{moment FechaRealizacion}}
             </center>
              {{/each}}
            </address>
          </div>

        </div>
      </div>
    </div>

 <div class="row mx-auto ">
<table class="table" style="font-size:x-large; margin-left: 10px; color: #000000; text-transform: uppercase; width:750px">
  <tbody>
    {{#each cliente}}
    <tr>
      <td><strong>Cliente</strong></td>
      <td>{{Nombre}}</td>
    </tr>
    <tr style="background-color: #dbdbdb;">
      <td><strong>Calle</strong></td>
      <td>{{DirCalle}} {{DirNum}}</td>
    </tr>
    <tr>
      <td><strong>Ubicacion</strong></td>
      <td>{{DirEntre}}</td>
    </tr>
    <tr style="background-color: #dbdbdb;">
      <td><strong>Colonia</strong></td>
      <td>{{DirColonia}}</td>
    </tr>
    <tr >
      <td><strong>Localidad</strong></td>
      <td>{{Municipio}}</td>
    </tr>
    <tr style="background-color: #dbdbdb;">
      <td><strong>Telefono</strong></td>
      <td>{{Telefono}}</td>
    </tr>
    <tr>
      <td><strong>Celular</strong></td>
      <td>{{Celular}}</td>
    </tr>
    {{/each}}
    {{#each equipo}}
    <tr style="background-color: #dbdbdb;">
      <td><strong>Aparato</strong></td>
      <td>{{Tipo}}</td>
    </tr>
    <tr>
      <td><strong>Marca</strong></td>
      <td>{{Marca}}</td>
    </tr>
    <tr style="background-color: #dbdbdb;">
      <td><strong>Modelo</strong></td>
      <td>{{Modelo}}</td>
    </tr>
    <tr>
      <td><strong>Serie</strong></td>
      <td>{{Serie}}</td>
    </tr>
    <br><br>
    {{/each}}
    {{#each datos}}
         <tr style="background-color: #dbdbdb;">
      <td><strong>Falla</strong></td>
      <td>{{Falla}}</td>
    </tr>
    <tr >
      <td><strong>Estado del Servicio</strong></td>
      <td>{{Realizado}}</td>
    </tr>
    <tr style="background-color: #dbdbdb;">
      <td><strong>Total</strong></td>
      <td>${{CostoServicio}}</td>
    </tr>
    <tr >
      <td><strong>Anticipo</strong></td>
      <td>${{Presupuesto}}</td>
    </tr>
    {{/each}}
    <tr style="background-color: #dbdbdb;">
      <td><strong>Restante</strong></td>
      <td>${{Restante}}</td>
    </tr>
    {{#each tecnico}}
    <tr >
      <td><strong>Tecnico</strong></td>
      <td>{{Nombre}}</td>
    </tr>
      {{/each}}
      {{#each datos}}
      <tr style="background-color: #dbdbdb;">
      <td><strong>Medio de difusion</strong></td>
      <td>{{MedioDeInformacion}}</td>
    </tr>
     <tr >
      <td><strong>Explicacion del servicio</strong></td>
      <td>{{Observaciones}}</td>
    </tr>
     <tr style="background-color: #dbdbdb;">
      <td><strong>Explicacion de Garantia</strong></td>
      <td>{{NotasGarantia}}</td>
    </tr>
    <tr >
      <td><strong>Dia</strong></td>
      <td style="color: red;"><strong>{{momentD FechaGarantiaNew}}</strong></td>
    </tr>
    <tr style="background-color: #dbdbdb;">
      <td><strong>Hora</strong></td>
      <td style="color: red;"><strong>{{HoraGarantia}}</strong></td>
    </tr>
    <tr >
      <td><strong>Observaciones</strong></td>
      <td>{{ObservacionesExtra}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
</div>
</div>
</div>
</div>
</div>